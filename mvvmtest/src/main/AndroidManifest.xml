<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.kiwilss.lxkj.mvvmtest">

    <application
            android:name=".MyApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:ignore="GoogleAppIndexingWarning,UnusedAttribute">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.HomeActivity" android:label="首页" android:screenOrientation="portrait"/>
        <activity android:name=".ui.webview.WebviewActivity" android:label="webview" android:screenOrientation="portrait"/>

        <activity android:name=".ui.login.LoginActivity" android:label="登录" android:screenOrientation="portrait"/>
        <activity android:name=".ui.collect.CollectActivity" android:label="收藏" android:screenOrientation="portrait"/>

        <activity android:name=".ui.todo.ToDoActivity" android:label="todo" android:screenOrientation="portrait"/>
        <activity android:name=".ui.add_todo.AddToDoActivity" android:label="@string/add_todo" android:screenOrientation="portrait"/>

        <activity android:name=".ui.search.SearchActivity" android:screenOrientation="portrait" android:label="@string/search"/>
        <activity android:name=".ui.search_list.SearchListActivity" android:label="@string/search" android:screenOrientation="portrait"/>

        <activity android:name=".ui.knowledge.detail.KnowledgeDetailActivity" android:screenOrientation="portrait" android:label="知识体系"/>

        <activity android:name=".ui.tucong.TuCongActivity" android:screenOrientation="portrait" android:label="图虫壁纸"/>
        <activity android:name=".ui.sister.SisterActivity" android:label="福利" android:screenOrientation="portrait"/>

        <activity android:name=".ui.setting.SettingActivity" android:screenOrientation="portrait" android:label="设置"/>
        <activity android:name=".ui.about.AboutActivity" android:label="@string/about_us" android:screenOrientation="portrait"/>





    </application>

</manifest>